<%= render 'pages/navheader' %>



<h1>Book Club</h1>

<%#= link_to 'New book', new_book_path %>


  <% @books.each do |book| %>
  <div class="jumbotron">
    <h2><%=book.title%></h2>
    <h3><%=book.text.html_safe%></h3>
  </div>

    <%#=link_to 'Show',book_path(book)%>
    <%#= link_to 'Edit', edit_book_path(book) %>
    <%#= link_to 'Destroy', book_path(book),
              method: :delete,
              data: { confirm: 'Are you sure?' } %>
  <%end%>
