

<h1>Podcasts</h1>


<button class="mdl-button mdl-js-button mdl-button--fab mdl-button--mini-fab">
  <i class="material-icons"><%= link_to "add", new_podcast_path%></i>
</button>



  <% @podcasts.each do |podcast| %>
    <%=podcast.title%>
    <%=podcast.text.html_safe%>
    <%=link_to 'Show',podcast_path(podcast)%>
    <%= link_to 'Edit', edit_podcast_path(podcast) %>
    <%= link_to 'Destroy', podcast_path(podcast),
              method: :delete,
              data: { confirm: 'Are you sure?' } %>
  <%end%>
